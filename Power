import java.util.*;

class Solution {
    public double myPow(double x, int n) 
    {
       double a=Math.pow(x,n); 
       return a;
    }
    public static void main(String []args)
    {
        Scanner sc=new Scanner(System.in);
        double x=sc.nextDouble();
        int n=sc.nextInt();
        System.out.println(new Solution().myPow(x,n));
    }
}
